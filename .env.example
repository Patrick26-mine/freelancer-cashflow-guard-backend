# Copy this file to .env and fill in values before running in real DB mode
# Do NOT commit your real .env to source control

# Postgres connection string (Supabase or other Postgres provider)
# Example: postgresql://postgres:password@db.host:6543/postgres
DATABASE_URL=

# Run the server in mock mode for local development (set to 'true')
USE_MOCK_DB=true

# Optional: server port
PORT=5001
